{
  "manifest_version": 3,
  "name": "YouTube Login Bypass (Embed Fallback)",
  "version": "1.1",
  "description": "Xóa overlay ép đăng nhập và tự chuyển sang embed khi gặp 'video không có sẵn'.",
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://www.youtube-nocookie.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["bypass.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["scripting"]
}
